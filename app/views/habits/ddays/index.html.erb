<p id="notice"><%= notice %></p>

<h1>Ddays</h1>

<table>
    <thead>
    <tr>
        <th>Project</th>
        <th>Day</th>
        <th>Success</th>
        <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @ddays.each do |dday| %>
        <tr>
            <td><%= @project %></td>
            <td><%= dday.day %></td>
            <td><%= dday.success %></td>
            <td><%= link_to 'Show', project_dday_path(@project, dday) %></td>
            <td><%= link_to 'Edit', edit_project_dday_path(@project, dday) %></td>
            <td><%= link_to 'Destroy', project_dday_path(@project, dday), method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
    </tbody>
</table>

<br>

<%= link_to 'New Dday', new_project_dday_path %>
